{
    "openapi": "3.0.0",
    "info": {
        "title": "API ElorAdmin",
        "version": "1.0"
    },
    "paths": {
        "/api/studies": {
            "get": {
                "tags": [
                    "Studies"
                ],
                "summary": "Mostrar modulos",
                "operationId": "23899138606040a842fd75f103acb29c",
                "responses": {
                    "200": {
                        "description": "Mostrar todos los modulos."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            },
            "post": {
                "tags": [
                    "Studies"
                ],
                "summary": "Guardar modulo",
                "operationId": "eb65b1712d9a6bcaf120fc5716e5efd8",
                "responses": {
                    "200": {
                        "description": "Guarda modulo nuevo."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "api/studies/{study}": {
            "get": {
                "tags": [
                    "Studies"
                ],
                "summary": "Enseña un modulo",
                "operationId": "97c0b465bf5d316fcc4bd248bbf592e6",
                "responses": {
                    "200": {
                        "description": "Enseña un modulo."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            },
            "delete": {
                "tags": [
                    "Studies"
                ],
                "summary": "Elimina un modulo",
                "operationId": "615ca072c7c500bdf714f691084d7160",
                "responses": {
                    "200": {
                        "description": "Elimina un modulo."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/studies/{study}": {
            "put": {
                "tags": [
                    "Studies"
                ],
                "summary": "Modifica un modulo",
                "operationId": "477949d8e5b8742dd04216f3cdb642a2",
                "responses": {
                    "200": {
                        "description": "Modificar un modulo."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/subjects": {
            "get": {
                "tags": [
                    "Subjects"
                ],
                "summary": "Listar asignaturas",
                "operationId": "034223809d31f97c85e65fb36d1bd29f",
                "responses": {
                    "200": {
                        "description": "Lista de asignaturas obtenida correctamente"
                    }
                }
            },
            "post": {
                "tags": [
                    "Subjects"
                ],
                "summary": "Crear una nueva asignatura",
                "operationId": "e2c447d20570f559a35f847a6be0b8f6",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Matemáticas"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Asignatura de Matemáticas."
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Asignatura creada correctamente"
                    }
                }
            }
        },
        "/api/subjects/{id}": {
            "get": {
                "tags": [
                    "Subjects"
                ],
                "summary": "Mostrar asignatura específica",
                "operationId": "453715d2b78a264a1ecdb77c0a016437",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la asignatura",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Asignatura obtenida correctamente"
                    },
                    "404": {
                        "description": "Asignatura no encontrada"
                    }
                }
            },
            "put": {
                "tags": [
                    "Subjects"
                ],
                "summary": "Actualizar asignatura",
                "operationId": "7c8c4c17a30f30aa89ce0f0613e6b850",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la asignatura",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Historia"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Asignatura de Historia."
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Asignatura actualizada correctamente"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Subjects"
                ],
                "summary": "Eliminar una asignatura",
                "operationId": "f55e964a3cbeb85012db401c5fef9e9d",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la asignatura",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Asignatura eliminada correctamente",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Subject deleted successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Asignatura no encontrada"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Study": {
                "title": "Study Model",
                "required": [
                    "name",
                    "description"
                ],
                "properties": {
                    "id": {
                        "description": "ID of the study",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "Name of the study",
                        "type": "string",
                        "example": "Computer Science"
                    },
                    "description": {
                        "description": "Description of the study",
                        "type": "string",
                        "example": "Study of computer science and related technologies"
                    },
                    "created_at": {
                        "description": "Timestamp when the study was created",
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-01-01T00:00:00Z"
                    },
                    "updated_at": {
                        "description": "Timestamp when the study was last updated",
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-01-15T00:00:00Z"
                    },
                    "deleted_at": {
                        "description": "Timestamp when the study was deleted (null if not deleted)",
                        "type": "string",
                        "format": "date-time",
                        "example": "null"
                    }
                },
                "type": "object"
            },
            "Subject": {
                "title": "Subject Model",
                "required": [
                    "name",
                    "description",
                    "study_id"
                ],
                "properties": {
                    "id": {
                        "description": "ID of the subject",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "Name of the subject",
                        "type": "string",
                        "example": "Mathematics"
                    },
                    "description": {
                        "description": "Description of the subject",
                        "type": "string",
                        "example": "Study of mathematical principles and concepts"
                    },
                    "study_id": {
                        "description": "ID of the associated study",
                        "type": "integer",
                        "example": 1
                    },
                    "created_at": {
                        "description": "Timestamp when the subject was created",
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-01-01T00:00:00Z"
                    },
                    "updated_at": {
                        "description": "Timestamp when the subject was last updated",
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-01-15T00:00:00Z"
                    },
                    "deleted_at": {
                        "description": "Timestamp when the subject was deleted (null if not deleted)",
                        "type": "string",
                        "format": "date-time",
                        "example": "null"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Studies",
            "description": "Operaciones relacionadas con los modulos"
        },
        {
            "name": "Subjects",
            "description": "Operaciones relacionadas con las asignaturas"
        }
    ]
}